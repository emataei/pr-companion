# AI Foundry Configuration for Local Testing
# Copy this to .env and fill in your actual values

# AI Foundry Endpoint (from your Azure AI deployment)
AI_FOUNDRY_ENDPOINT=https://your-resource-name.openai.azure.com

# AI Foundry API Key/Token
AI_FOUNDRY_TOKEN=your_api_key_here

# Model deployment name in Azure AI
AI_FOUNDRY_MODEL=gpt-4

# GitHub context for testing
GITHUB_TOKEN=your_github_token_here

# Test PR context
PR_NUMBER=123
CHANGED_FILES=["src/components/UserProfile.tsx","src/app/page.tsx"]
